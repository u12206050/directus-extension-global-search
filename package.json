{
	"name": "directus-extension-global-search",
	"type": "module",
	"version": "2.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-module"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		  },
		"source": "src/index.ts",
		"host": "^10.0.0",
		"entries": [
			{
			  "type": "module",
			  "name": "module",
			  "source": "src/modules/index.ts"
			}
		  ]
	},
	"scripts": {
		"dev": "directus-extension build -w --no-minify",
		"build": "directus-extension build"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "^11.0.2",
		"typescript": "^5.4.4",
		"vue": "^3.4.21"
	},
	"dependencies": {
		"@directus/utils": "^11.0.7"
	}
}
